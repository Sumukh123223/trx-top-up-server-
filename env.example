# Railway TRON Scanner Backend Configuration

# Railway automatically sets PORT, don't change this
PORT=3000

# TRON Configuration - YOUR SERVER WALLET
TRON_PRIVATE_KEY=your_private_key_here
TRON_ADDRESS=your_tron_address_here

# Auto-send Configuration
AUTO_SEND_AMOUNT=17
MINIMUM_BALANCE=1

# Security
NODE_ENV=production

# CORS Origins (add your frontend domains)
ALLOWED_ORIGINS=https://208606ac73dc.ngrok-free.app,https://your-frontend-domain.com

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100
